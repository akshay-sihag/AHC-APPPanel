{
  "crons": [
    {
      "path": "/api/cron/auto-backup",
      "schedule": "0 6 * * *"
    },
    {
      "path": "/api/cron/process-scheduled-notifications",
      "schedule": "0 * * * *"
    },
    {
      "path": "/api/cron/process-deletion-requests",
      "schedule": "0 * * * *"
    },
    {
      "path": "/api/cron/resume-notifications",
      "schedule": "*/5 * * * *"
    },
    {
      "path": "/api/cron/cleanup-push-logs",
      "schedule": "0 * * * *"
    }
  ]
}
